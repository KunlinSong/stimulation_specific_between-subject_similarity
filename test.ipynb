    "import numpy as np\n",
    "\n",
    "a = np.arange(1, 10).reshape(3, 3).astype(float)\n",
    "a[1, 1] = np.nan\n",
    "rg = np.random.default_rng(1)\n",
    "nni = np.nonzero(~np.isnan(a))\n",
    "ri = np.stack(nni, axis=0)\n",
    "rg.shuffle(ri, axis=1)\n",
    "mri = tuple([i for i in ri])\n",
    "a[nni] = a[mri]\n",
    "a"
   ]
